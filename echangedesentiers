#include <stdio.h>

int main(void)
{
    int ENTIER1, ENTIER2;
    int ECHANGE;
    printf("valeur du premier entier :");
    //designation des entiers
    scanf("%d", &ENTIER1);
    printf("valeur du deuxieme entier:");
    scanf("%d", &ENTIER2);
    //tri
    ECHANGE=ENTIER1; ENTIER1=ENTIER2;ENTIER2=ECHANGE;
    //enchange des entiers
    printf("Apres Ã©change, le 1er entier = %d, et le deuxieme = %d", ENTIER1, ENTIER2);
}
